<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExtendedTextView">
        <attr name="fontName" format="string" />
        <attr name="fontFormat" format="string" />
        <attr name="required" format="boolean" />
        <attr name="trimmed" format="boolean" />
        <attr name="validEmail" format="boolean" />
        <attr name="matchValueOf" format="reference|integer" />
        <attr name="minLength" format="integer" />
    </declare-styleable>
    <declare-styleable name="AppCompatTextView">
        <attr name="textAllCaps" format="reference|boolean" />
        <attr name="android:textAppearance" />
    </declare-styleable>
    <declare-styleable name="TextAppearance">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:textStyle" />
        <attr name="android:typeface" />
        <attr name="textAllCaps" />
    </declare-styleable>
</resources>